<pattern id="Dreal_modified" name="DREAL data modified with Excel into .prn">
    <structure_type>constant_width</structure_type>
    <header_lines>1</header_lines>
    <parse_time>1</parse_time>
    <time>
        <block char_start="1" char_end="4">year</block>
        <block char_start="5" char_end="6">month</block>
        <block char_start="7" char_end="8">day</block>
    </time>
    <data>
        <block char_start="11" char_end="16">precipitation</block>
    </data>
</pattern>
